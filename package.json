{
  "name": "@djforth/gmaps_helper",
  "version": "1.3.0",
  "description": "Gmaps Helper for quick set up",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production babel src -x '.js'  -d lib",
    "build:watch": "babel src -x '.js' -w --source-maps  -d lib",
    "lint": "eslint src/** --fix",
    "lint:tests": "eslint spec/**  --fix",
    "server": "node ./webpack/server.js",
    "test": "./node_modules/@djforth/ap_karma_wp/bin/server.js",
    "test:mac": "asset-pipeline-karma -b",
    "test:watch": "asset-pipeline-karma -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/djforth/gmaps_helper.git"
  },
  "keywords": [
    "Gmaps",
    "Google",
    "maps"
  ],
  "author": "A Stainforth",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/djforth/gmaps_helper/issues"
  },
  "homepage": "https://github.com/djforth/gmaps_helper#readme",
  "devDependencies": {
    "@djforth/ap_karma_wp": "^2.0.2",
    "@djforth/ap_utils": "^3.0.4",
    "@djforth/morse-jasmine-wp": "^2.4.2",
    "babel": "^6.23.0",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-rewire": "^1.1.0",
    "babel-plugin-rewire-ignore-coverage": "^2.0.2",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-plugin-lodash": "^3.2.11",
    "babel-preset-babili": "^0.0.12",
    "babel-preset-env": "^1.4.0",
    "eslint": "^3.19.0",
    "eslint-config-morsedigital": "^1.2.3",
    "eslint-plugin-jasmine": "^2.2.0",
    "jasmine-core": "^2.4.1",
    "lodash": "^4.17.4",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  },
  "peerDependencies": {
    "lodash": ">=4.17.4"
  },
