{
  "name": "@djforth/gmaps_helper",
  "version": "1.3.0",
  "description": "Gmaps Helper for quick set up",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production babel src -x '.js'  -d lib",
    "build:watch": "babel src -x '.js' -w --source-maps  -d lib",
    "lint": "eslint src/** --fix",
    "lint:tests": "eslint spec/**  --fix",
    "server": "node ./webpack/server.js",
    "test": "./node_modules/@djforth/ap_karma_wp/bin/server.js",
    "test:mac": "asset-pipeline-karma -b",
    "test:watch": "asset-pipeline-karma -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/djforth/gmaps_helper.git"
  },
  "keywords": [
    "Gmaps",
    "Google",
    "maps"
  ],
  "author": "A Stainforth",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/djforth/gmaps_helper/issues"
  },
  "homepage": "https://github.com/djforth/gmaps_helper#readme",
  "devDependencies": {
    "@djforth/ap_karma_wp": "^3.0.2",
    "@djforth/ap_utils": "^3.1.2",
    "@djforth/morse-jasmine-wp": "^2.5.0",
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-rewire": "^1.1.0",
    "babel-plugin-rewire-ignore-coverage": "^2.0.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-minify": "^0.2.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.9.0",
    "eslint-config-morsedigital": "^2.1.1",
    "eslint-plugin-jasmine": "^2.9.1",
    "jasmine-core": "^2.8.0",
    "lodash": "^4.17.4",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.2"
  },
  "dependencies": {
    "lodash": ">=4.17.4"
  },
  "dependencies": {}
}
