{
  "name": "@djforth/gmaps_helper",
  "version": "1.1.2",
  "description": "Gmaps Helper for quick set up",
  "main": "index.js",
  "scripts": {
    "build": "babel src -x '.js'  -d lib",
    "build:watch": "babel src -x '.js' -w --source-maps  -d lib",
    "lint": "eslint src/**",
    "lint:tests": "eslint spec/**",
    "test": "npm run lint && ./node_modules/.bin/karma start --browsers PhantomJS --single-run",
    "test:watch": "karma start --browsers PhantomJS",
    "test:mac": "karma start --single-run --browsers Chrome,Firefox,Safari"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/djforth/gmaps_helper.git"
  },
  "keywords": [
    "Gmaps",
    "Google",
    "maps"
  ],
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "morsedigital"
  },
  "assets": {
    "assets_in": "./src",
    "assets_out": "./lib",
    "javascripts": {
      "transforms": [
        [
          "babelify",
          {
            "presets": [
              "es2015"
            ]
          }
        ]
      ]
    },
    "karma": {
      "input": "./spec"
    }
  },
  "author": "A Stainforth",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/djforth/gmaps_helper/issues"
  },
  "homepage": "https://github.com/djforth/gmaps_helper#readme",
  "devDependencies": {
    "@djforth/ap_karma": "^1.1.11",
    "@djforth/ap_utils": "^2.0.13",
    "@djforth/morse-jasmine": "1.0.2",
    "babel": "^6.5.2",
    "babel-cli": "^6.6.0",
    "babel-eslint": "^5.0.0",
    "babel-polyfill": "^6.6.1",
    "babel-preset-es2015": "^6.6.0",
    "eslint-config-morsedigital": "^1.0.1",
    "jasmine-core": "^2.4.1",
    "lodash": "~4.8.2"
  },
  "peerDependencies": {
    "lodash": "~4.8.2"
  }
}
