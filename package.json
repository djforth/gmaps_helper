{
  "name": "@djforth/gmaps_helper",
  "version": "2.1.0",
  "description": "Gmaps Helper for quick set up",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production rollup -c",
    "build:watch": "NODE_ENV=production rollup -c -w",
    "lint": "eslint src/** --fix",
    "lint:tests": "eslint __tests__/** --fix",
    "start": "NODE_ENV=development webpack-dev-server --progress --color --config config/webpack.config.js",
    "test": "jest --config jest.config.json",
    "test:watch": "jest --watch --config jest.config.json",
    "webpack": "NODE_ENV=development webpack --progress --color --display-depth --config config/webpack.config.js",
    "webpack:watch": "NODE_ENV=development webpack -w --progress --color --display-depth --config config/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/djforth/gmaps_helper.git"
  },
  "keywords": [
    "Gmaps",
    "Google",
    "maps"
  ],
  "author": "A Stainforth",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/djforth/gmaps_helper/issues"
  },
  "homepage": "https://github.com/djforth/gmaps_helper#readme",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-destructuring": "^7.5.0",
    "@babel/plugin-transform-regenerator": "^7.4.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "@djforth/jest-matchers": "2.0.0-0",
    "babel-core": "^7.0.0-bridge",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "babel-plugin-istanbul": "5.2.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-preset-es2015-rollup": "^3.0.0",
    "eslint-config-morsedigital-prettier": "^2.1.0",
    "eslint-plugin-jest": "^22.14.0",
    "jest": "^24.8.0",
    "lodash": "^4.17.15",
    "rollup": "^1.17.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-uglify": "^6.0.2"
  }
}
