{
  "name": "@djforth/gmaps_helper",
  "version": "1.1.3",
  "description": "Gmaps Helper for quick set up",
  "main": "index.js",
  "scripts": {
    "build": "babel src -x '.js'  -d lib",
    "build:watch": "babel src -x '.js' -w --source-maps  -d lib",
    "lint": "eslint src/** --fix",
    "lint:tests": "eslint spec/**  --fix",
    "server": "node ./webpack/server.js",
    "test": "./node_modules/@djforth/ap_karma_wp/bin/server.js",
    "test:mac": "asset-pipeline-karma -b",
    "test:watch": "asset-pipeline-karma -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/djforth/gmaps_helper.git"
  },
  "keywords": [
    "Gmaps",
    "Google",
    "maps"
  ],
  "author": "A Stainforth",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/djforth/gmaps_helper/issues"
  },
  "homepage": "https://github.com/djforth/gmaps_helper#readme",
  "devDependencies": {
    "@djforth/ap_karma_wp": "^1.0.6",
    "@djforth/ap_utils": "^3.0.1",
    "@djforth/morse-jasmine-wp": "^2.4.2",
    "babel": "^6.5.2",
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-rewire": "^1.0.0",
    "babel-plugin-rewire-ignore-coverage": "^2.0.2",
    "babel-polyfill": "^6.6.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2016": "^6.16.0",
    "babel-preset-es2017": "^6.16.0",
    "eslint": "^3.13.1",
    "eslint-config-morsedigital": "^1.0.1",
    "eslint-plugin-jasmine": "^2.2.0",
    "jasmine-core": "^2.4.1",
    "lodash": "^4.17.4",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-dev-server": "^1.16.2",
    "webpack-hot-middleware": "^2.15.0"
  },
  "peerDependencies": {
    "lodash": "^4.*.*"
  }
}
